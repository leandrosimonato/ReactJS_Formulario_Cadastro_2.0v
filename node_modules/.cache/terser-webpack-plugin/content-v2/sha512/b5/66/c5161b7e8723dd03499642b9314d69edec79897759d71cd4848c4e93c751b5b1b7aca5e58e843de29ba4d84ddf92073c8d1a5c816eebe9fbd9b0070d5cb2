={\n          <Switch\n            checked={novidades}\n            onChange={(event) => {\n              setNovidades(event.target.checked);\n            }}\n            name=\"novidades\"\n            color=\"primary\"\n          />\n        }\n      />\n\n      <Button type=\"submit\" variant=\"contained\" color=\"primary\">\n        Cadastrar\n      </Button>\n    </form>\n  );\n}\n\nexport default DadosPessoais;","import React from \"react\";\nimport { TextField, Button } from \"@material-ui/core\";\n\nfunction DadosUsuario({ aoEnviar }) {\n  return (\n    <form onSubmit={(event) => {\n      event.preventDefault();\n      aoEnviar();\n    }}>\n      <TextField\n        id=\"email\"\n        label=\"email\"\n        type=\"email\"\n        required\n        variant=\"outlined\"\n        margin=\"normal\"\n        fullWidth\n      />\n      <TextField\n        id=\"senha\"\n        label=\"senha\"\n        type=\"password\"\n        required\n        variant=\"outlined\"\n        margin=\"normal\"\n        fullWidth\n      />\n      <Button type=\"submit\" variant=\"contained\" color=\"primary\">\n        Cadastrar\n      </Button>\n    </form>\n  );\n}\n\nexport default DadosUsuario;","import React from \"react\";\nimport { TextField, Button } from \"@material-ui/core\";\n\nfunction DadosEntrega() {\n  \n  return (\n    <form>\n      <TextField\n        id=\"cep\"\n          label=\"CEP\"\n            type=\"number\"\n          variant=\"outlined\"\n        margin=\"normal\"\n    \n      />\n      <TextField\n          id=\"endereco\"\n            label=\"Enderço\"\n              type=\"text\"\n            variant=\"outlined\"\n          margin=\"normal\"\n        fullWidth\n      />\n       <TextField\n        id=\"numero\"\n          label=\"Numero\"\n            type=\"number\"\n          variant=\"outlined\"\n        margin=\"normal\"\n        \n      />\n       <TextField\n        id=\"estado\"\n          label=\"Estado\"\n            type=\"text\"\n       